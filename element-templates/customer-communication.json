{
  "$schema": "https://unpkg.com/@camunda/zeebe-element-templates-json-schema/resources/schema.json",
  "name": "Customer Communication",
  "id": "CustomerCommunication",
  "version": 1,
  "description": "",
  "category": {
    "id": "templates",
    "name": "Templates"
  },
  "icon": {
    "contents": "data:image/svg+xml;base64,"
  },
  "appliesTo": [
    "bpmn:CallActivity"
  ],
"properties": [
    {
      "label": "Called process (processId)",
      "type": "Hidden",
      "binding": {
        "type": "zeebe:calledElement",
        "property": "processId"
      },
      "value": "customer-communication"
    },
    {
      "label": "Communication Context",
      "type": "String",
      "binding": {
        "type": "zeebe:input",
        "name": "communicationContext"
      },
      "optional": true,
      "feel": "optional",
      "value": ""
    },
    {
      "label": "Communication Content",
      "type": "String",
      "binding": {
        "type": "zeebe:input",
        "name": "communicationContent"
      },
      "optional": true,
      "feel": "optional"
    },
    {
      "label": "Response variable name",
      "type": "String",
      "binding": {
        "type": "zeebe:output",
        "source": "=response"
      },
      "feel": "optional",
      "value": "communicationResponse"
    }
  ]  
}