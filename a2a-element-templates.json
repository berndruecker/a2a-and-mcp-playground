[{
  "$schema": "https://unpkg.com/@camunda/zeebe-element-templates-json-schema/resources/schema.json",
  "name": "A2A Server (Inbound)",
  "id": "dev.example.a2a.connector.inbound",
  "version": 1,
  "description": "Expose agent via A2A",
  "category": {
    "id": "templates",
    "name": "Templates"
  },
  "icon": {
    "contents": "data:image/svg+xml;base64,"
  },
  "appliesTo": [
    "bpmn:StartEvent"
  ],
  "properties": [
    {
      "type": "Hidden",
      "value": "io.example:a2a:1",
      "binding": { "type": "zeebe:property", "name": "inbound.type" }
    },
    {
      "label": "Agent ID",
      "type": "String",
      "binding": { "type": "zeebe:property", "name": "agentId" }
    },
    {
      "label": "Skills",
      "description": "Comma seperated list of skills",
      "type": "Text",
      "feel": "optional",
      "binding": { "type": "zeebe:property", "name": "skills" }
    },
    {
      "label": "Result variable",
      "binding": { "type": "zeebe:property", "name": "resultVariable" },
      "value": "input"
    }
  ]
},
{
  "$schema": "https://unpkg.com/@camunda/zeebe-element-templates-json-schema/resources/schema.json",
  "name": "A2A Client (Outbound)",
  "id": "dev.example.a2a.connector.outbound",
  "version": 1,
  "description": "Expose agent via A2A",
  "category": {
    "id": "templates",
    "name": "Templates"
  },
   "icon": {
    "contents": "data:image/svg+xml;base64,"
  },
 "appliesTo": ["bpmn:ServiceTask"],
 "entriesVisible": true,
  "properties": [
    {
      "label": "Connector Type",
      "type": "Hidden",
      "value": "dev.example:a2a:1",
      "binding": { "type": "zeebe:taskDefinition:type" }
    },
    {
      "label": "Base URL",
      "type": "String",
      "description": "e.g. http://localhost:8000",
      "binding": { "type": "zeebe:input", "name": "baseUrl" }
    },
    {
      "label": "Mode",
      "type": "Dropdown",
      "choices": [{ "name": "REST", "value": "REST" }, { "name": "A2A", "value": "A2A" }],
      "binding": { "type": "zeebe:input", "name": "mode" },
      "value": "REST"
    },
    {
      "label": "Input Text",
      "type": "String",
      "description": "Natural-language request to the agent",
      "binding": { "type": "zeebe:input", "name": "inputText" }
    },
    {
      "label": "Intent",
      "type": "String",
      "description": "Optional high-level intent, e.g. report_lost",
      "binding": { "type": "zeebe:input", "name": "intent" }
    },
    {
      "label": "Parameters (JSON)",
      "type": "Text",
      "binding": { "type": "zeebe:input", "name": "parameters" }
    },
    {
      "label": "Headers (JSON)",
      "type": "Text",
      "binding": { "type": "zeebe:input", "name": "headers" }
    },
    {
      "label": "A2A Payload (JSON)",
      "type": "Text",
      "feel": "optional",      
      "description": "Advanced: raw JSON for A2A mode",
      "binding": { "type": "zeebe:input", "name": "a2aPayload" }
    }
  ]
}
]